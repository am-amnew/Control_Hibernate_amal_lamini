<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.Etudiant" table="Etudiant">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="identity" />
        </id>
        
        <property name="nom" type="java.lang.String" column="NOM" />
        <property name="password1" type="string" column="PASSWORD" />
        <property name="email" type="java.lang.String" column="EMAIL" />
        <property name="tel" type="java.lang.String" column="TEL" />
        <property name="ecole" type="java.lang.String" column="ECOLE" />
    </class>
</hibernate-mapping>
