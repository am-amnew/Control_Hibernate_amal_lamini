<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bean.Module" table="Module">
        <id name="moduleId" type="java.lang.Integer">
            <column name="MODULE_ID" />
            <generator class="identity" />
        </id>
        <property name="moduleName" type="java.lang.String" column="MODULE_NAME" />
        <property name="moduleDescription" type="java.lang.String" column="MODULE_DESCRIPTION" />
       

    </class>
</hibernate-mapping>
